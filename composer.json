{
    "name": "iemand002/filemanager",
    "description": "Multi language file manager package",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Ruud Lamers",
            "email": "ruud.lamers@live.nl"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/config": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "intervention/image": "^2.4",
        "dflydev/apache-mime-types": "^1.0",
        "laravel/socialite": "^3.0",
        "socialiteproviders/dropbox": "^4.0",
        "socialiteproviders/microsoft-graph": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Iemand002\\Filemanager\\": "src/Filemanager"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iemand002\\Filemanager\\FilemanagerServiceProvider"
            ],
            "aliases": {
                "Filemanager": "Iemand002\\Filemanager\\FilemanagerFacade"
            },
            "dont-discover": [
                "laravel/socialite"
            ]
        }
    }
}
